<%= form_for [current_user, @main_address] do |f| %>
<ul>
   <%= f.label :address %> 
   <%= f.text_field :address %>
   <%= f.label :city %>  
   <%= f.text_field :city %>
   <%= f.label :state %> 
   <%= f.text_field :state %>
   <%= f.label :zip %> 
   <%= f.text_field :zip %>
   <%= f.submit "Submit"%>
<ul>
<% end %>
